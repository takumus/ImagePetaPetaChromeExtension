{
  "name": "image-peta-peta-chrome-extension",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@types/chrome": "^0.0.268",
    "@types/node": "20.12.12",
    "@types/recursive-readdir": "^2.2.4",
    "@types/throttle-debounce": "^5.0.2",
    "@vitejs/plugin-vue": "^5.0.4",
    "keypress": "^0.2.1",
    "license-checker-rseidelsohn": "^4.3.0",
    "npm-check-updates": "^16.14.20",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "prettier-plugin-css-order": "^2.1.2",
    "pretty-quick": "^4.0.0",
    "recursive-readdir": "^2.2.3",
    "sass": "^1.77.2",
    "tree-kill": "^1.2.2",
    "tsx": "^4.11.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.11"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:scripts": "vite build --config vite.scripts.config.ts",
    "build:popup": "vite build --config vite.popup.config.ts",
    "build:manifest": "tsx ./scripts/buildManifest.ts",
    "dev": "tsx ./scripts/dev.ts",
    "dev:scripts": "vite build --watch --config vite.scripts.config.ts",
    "dev:popup": "vite build --watch --config vite.popup.config.ts",
    "dev:manifest": "tsx watch ./scripts/buildManifest.ts",
    "generate": "run-p generate:*",
    "generate:dir": "tsx ./scripts/generateDir.ts",
    "generate:icon": "tsx ./scripts/buildIcon.ts",
    "predev": "npm run generate",
    "prebuild": "npm run generate",
    "format": "prettier --write \"**/*.{ts,html}\"",
    "check-updates": "npm-check-updates",
    "check-updates-u": "npm-check-updates -u"
  },
  "author": "",
  "volta": {
    "node": "22.1.0",
    "npm": "10.7.0"
  },
  "dependencies": {
    "file-type": "^19.0.0",
    "throttle-debounce": "^5.0.0",
    "url-join": "^5.0.0",
    "vue": "^3.4.27",
    "imagepetapeta-beta": "git+ssh://git@github.com/takumus/ImagePetaPeta.git#36016c36db67bdbe57f8fde4c1bedd06e7d1e1a1"
  }
